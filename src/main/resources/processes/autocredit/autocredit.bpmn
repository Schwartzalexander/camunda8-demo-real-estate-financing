<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:zeebe="http://camunda.org/schema/zeebe/1.0"
                  id="Definitions_RealEstateAutoCredit"
                  targetNamespace="http://example.com/real-estate">
  <bpmn:process id="RealEstateAutoCredit" name="Real estate auto credit" isExecutable="true">
    <bpmn:startEvent id="StartEvent_CreditApplication" name="Auto credit started">
      <bpmn:outgoing>Flow_StartToOffer</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Activity_GetCheapestOffer" name="Get cheapest offer">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="get-cheapest-offer" retries="10" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_StartToOffer</bpmn:incoming>
      <bpmn:outgoing>Flow_OfferToPdf</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Activity_CreateContractPDF" name="Create contract PDF">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-contract-pdf" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_OfferToPdf</bpmn:incoming>
      <bpmn:outgoing>Flow_PdfToEnd</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_ContractConcluded" name="Credit contract concluded">
      <bpmn:incoming>Flow_PdfToEnd</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_StartToOffer" sourceRef="StartEvent_CreditApplication" targetRef="Activity_GetCheapestOffer" />
    <bpmn:sequenceFlow id="Flow_OfferToPdf" sourceRef="Activity_GetCheapestOffer" targetRef="Activity_CreateContractPDF" />
    <bpmn:sequenceFlow id="Flow_PdfToEnd" sourceRef="Activity_CreateContractPDF" targetRef="EndEvent_ContractConcluded" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_AutoCredit">
    <bpmndi:BPMNPlane id="BPMNPlane_AutoCredit" bpmnElement="RealEstateAutoCredit">
      <bpmndi:BPMNShape id="Shape_StartEvent_AutoCredit" bpmnElement="StartEvent_CreditApplication">
        <dc:Bounds x="180" y="102" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Shape_Task_GetOffer" bpmnElement="Activity_GetCheapestOffer">
        <dc:Bounds x="270" y="80" width="140" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Shape_Task_CreatePdf" bpmnElement="Activity_CreateContractPDF">
        <dc:Bounds x="450" y="80" width="140" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Shape_EndEvent_AutoCredit" bpmnElement="EndEvent_ContractConcluded">
        <dc:Bounds x="640" y="102" width="36" height="36" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Edge_Flow_StartToOffer" bpmnElement="Flow_StartToOffer">
        <di:waypoint x="216" y="120" />
        <di:waypoint x="270" y="120" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_OfferToPdf" bpmnElement="Flow_OfferToPdf">
        <di:waypoint x="410" y="120" />
        <di:waypoint x="450" y="120" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Edge_Flow_PdfToEnd" bpmnElement="Flow_PdfToEnd">
        <di:waypoint x="590" y="120" />
        <di:waypoint x="640" y="120" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
