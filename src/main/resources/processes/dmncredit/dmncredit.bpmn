    <bpmn:sequenceFlow id="Flow_0ivbbal" name="Yes" sourceRef="Gateway_0j7bs3o" targetRef="Activity_CreateContractPDF">
      <bpmn:extensionElements>
        <camunda:executionListener class="de.aschwartz.camunda7demo.realestatefinancing.camunda.executionlistener.CreateRandomCheapestOffer" event="take" />
      </bpmn:extensionElements>
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="Event_1k1tpii" name="No contract">
      <bpmn:incoming>Flow_0mx3jhm</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0mx3jhm" name="No" sourceRef="Gateway_0j7bs3o" targetRef="Event_1k1tpii">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!decision}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
  </bpmn:process>
