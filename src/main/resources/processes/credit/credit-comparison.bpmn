<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
				  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
				  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
				  xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_MaDn_Game"
				  targetNamespace="http://example.com/mensch-aerger-dich-nicht" exporter="Camunda Modeler"
				  exporterVersion="5.42.0">
	<bpmn:process id="Call_Credit_Comparison" name="Real estate credit comparison" isExecutable="true">
		<bpmn:startEvent id="StartEvent_CreditApplication" name="User starts credit comparison">
			<bpmn:outgoing>Flow_0ed4lnc</bpmn:outgoing>
		</bpmn:startEvent>
		<bpmn:parallelGateway id="Gateway_RequestConditionsAllBanks" name="Request conditions from all banks">
			<bpmn:incoming>Flow_0ed4lnc</bpmn:incoming>
			<bpmn:outgoing>Flow_0ufmh5j</bpmn:outgoing>
			<bpmn:outgoing>Flow_0icz4vr</bpmn:outgoing>
			<bpmn:outgoing>Flow_0sr4cks</bpmn:outgoing>
		</bpmn:parallelGateway>
		<bpmn:sequenceFlow id="Flow_0ed4lnc" sourceRef="StartEvent_CreditApplication"
						   targetRef="Gateway_RequestConditionsAllBanks"/>
		<bpmn:serviceTask id="Task_RequestConditionsBankA" name="Request conditions Bank A"
						  camunda:delegateExpression="${requestConditionsBankADelegate}">
			<bpmn:incoming>Flow_0ufmh5j</bpmn:incoming>
			<bpmn:outgoing>Flow_1xus38j</bpmn:outgoing>
		</bpmn:serviceTask>
		<bpmn:sequenceFlow id="Flow_0ufmh5j" sourceRef="Gateway_RequestConditionsAllBanks"
						   targetRef="Task_RequestConditionsBankA"/>
		<bpmn:sequenceFlow id="Flow_1xus38j" sourceRef="Task_RequestConditionsBankA" targetRef="Gateway_JoinAllBanks"/>
		<bpmn:serviceTask id="Task_RequestConditionsBankB" name="Request conditions Bank B"
						  camunda:delegateExpression="${requestConditionsBankBDelegate}">
			<bpmn:incoming>Flow_0icz4vr</bpmn:incoming>
			<bpmn:outgoing>Flow_1s08u02</bpmn:outgoing>
		</bpmn:serviceTask>
		<bpmn:sequenceFlow id="Flow_0icz4vr" sourceRef="Gateway_RequestConditionsAllBanks"
						   targetRef="Task_RequestConditionsBankB"/>
		<bpmn:serviceTask id="Task_RequestConditionsBankC" name="Request conditions Bank C"
						  camunda:delegateExpression="${requestConditionsBankCDelegate}">
			<bpmn:incoming>Flow_0sr4cks</bpmn:incoming>
			<bpmn:outgoing>Flow_0sklhjc</bpmn:outgoing>
		</bpmn:serviceTask>
		<bpmn:sequenceFlow id="Flow_0sr4cks" sourceRef="Gateway_RequestConditionsAllBanks"
						   targetRef="Task_RequestConditionsBankC"/>
		<bpmn:sequenceFlow id="Flow_1s08u02" sourceRef="Task_RequestConditionsBankB" targetRef="Gateway_JoinAllBanks"/>
		<bpmn:sequenceFlow id="Flow_0sklhjc" sourceRef="Task_RequestConditionsBankC" targetRef="Gateway_JoinAllBanks"/>
		<bpmn:endEvent id="EndEvent_ComparisonFinished" name="Credit comparison finished">
			<bpmn:incoming>Flow_0qj4533</bpmn:incoming>
		</bpmn:endEvent>
		<bpmn:sequenceFlow id="Flow_0qj4533" sourceRef="Task_CollectResults" targetRef="EndEvent_ComparisonFinished"/>
		<bpmn:parallelGateway id="Gateway_JoinAllBanks" name="Join all bank results">
			<bpmn:incoming>Flow_0sklhjc</bpmn:incoming>
			<bpmn:incoming>Flow_1xus38j</bpmn:incoming>
			<bpmn:incoming>Flow_1s08u02</bpmn:incoming>
			<bpmn:outgoing>Flow_0q8jwl3</bpmn:outgoing>
		</bpmn:parallelGateway>
		<bpmn:sequenceFlow id="Flow_0q8jwl3" sourceRef="Gateway_JoinAllBanks" targetRef="Task_CollectResults"/>
		<bpmn:serviceTask id="Task_CollectResults" name="Collect results"
						  camunda:delegateExpression="${collectResultsDelegate}">
			<bpmn:incoming>Flow_0q8jwl3</bpmn:incoming>
			<bpmn:outgoing>Flow_0qj4533</bpmn:outgoing>
		</bpmn:serviceTask>
	</bpmn:process>
