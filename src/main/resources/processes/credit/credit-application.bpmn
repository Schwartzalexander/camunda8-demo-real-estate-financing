  <bpmn:process id="RealEstateCreditApplication" name="Real estate credit application" isExecutable="true">
    <bpmn:startEvent id="StartEvent_CreditApplication" name="User starts credit application">
      <bpmn:outgoing>Flow_1ta8xbz</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_ContractConcluded" name="Credit contract concluded">
      <bpmn:incoming>Flow_1jptx6s</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_ApplicationNotAccepted" name="No" sourceRef="Gateway_ApplicationReview" targetRef="EndEvent_ContractCancelled">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${applicationAccepted != true}
			</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:callActivity id="Call_Credit_Comparison" name="Credit comparison" calledElement="Call_Credit_Comparison">
      <bpmn:extensionElements>
        <camunda:in source="monthlyNetIncome" target="monthlyNetIncome" />
        <camunda:in source="propertyValue" target="propertyValue" />
        <camunda:in source="equity" target="equity" />
        <camunda:out source="creditOffers" target="creditOffers" />
        <camunda:in source="userId" target="userId" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0vysej9</bpmn:incoming>
      <bpmn:outgoing>Flow_12a2vnb</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:sequenceFlow id="Flow_0vysej9" sourceRef="Task_EnterCreditParameters" targetRef="Call_Credit_Comparison" />
    <bpmn:userTask id="Task_BankSelection" name="User selects bank">
      <bpmn:incoming>Flow_12a2vnb</bpmn:incoming>
      <bpmn:outgoing>Flow_194lp6r</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_12a2vnb" sourceRef="Call_Credit_Comparison" targetRef="Task_BankSelection" />
    <bpmn:userTask id="Task_SubmitApplication" name="User submits application">
      <bpmn:incoming>Flow_194lp6r</bpmn:incoming>
      <bpmn:outgoing>Flow_1bqavv7</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_194lp6r" sourceRef="Task_BankSelection" targetRef="Task_SubmitApplication" />
    <bpmn:exclusiveGateway id="Gateway_ApplicationReview" name="Bank accepts application">
      <bpmn:incoming>Flow_1537eeg</bpmn:incoming>
      <bpmn:outgoing>Flow_ApplicationNotAccepted</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_1537eeg" sourceRef="Task_ReviewCreditApplication" targetRef="Gateway_ApplicationReview" />
    <bpmn:serviceTask id="Task_ReviewCreditApplication" name="Bank reviews credit application" camunda:delegateExpression="${reviewCreditApplicationDelegate}">
      <bpmn:incoming>Flow_1bqavv7</bpmn:incoming>
      <bpmn:outgoing>Flow_1537eeg</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_1bqavv7" sourceRef="Task_SubmitApplication" targetRef="Task_ReviewCreditApplication" />
    <bpmn:serviceTask id="Task_SendContract" name="Bank sends signed contract" camunda:delegateExpression="${sendContractDelegate}">
      <bpmn:outgoing>Flow_0bwcnok</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ApplicationAccepted" name="Yes" sourceRef="Gateway_ApplicationReview" targetRef="Task_SendContract">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${applicationAccepted == true}
			</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Task_SignContract" name="User signs contract">
      <bpmn:incoming>Flow_0bwcnok</bpmn:incoming>
      <bpmn:outgoing>Flow_1jptx6s</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0bwcnok" sourceRef="Task_SendContract" targetRef="Task_SignContract" />
    <bpmn:sequenceFlow id="Flow_1jptx6s" sourceRef="Task_SignContract" targetRef="EndEvent_ContractConcluded" />
    <bpmn:endEvent id="EndEvent_ContractCancelled" name="Credit contract cancelled">
      <bpmn:incoming>Flow_ApplicationNotAccepted</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="Task_EnterCreditParameters" name="User enters credit parameters">
      <bpmn:incoming>Flow_1ta8xbz</bpmn:incoming>
      <bpmn:outgoing>Flow_0vysej9</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_1ta8xbz" sourceRef="StartEvent_CreditApplication" targetRef="Task_EnterCreditParameters" />
  </bpmn:process>
